{
  "moduleFileExtensions": ["js", "json", "ts"],
  "moduleDirectories": [
    "node_modules",
    "<rootDir>"
  ],
  "moduleNameMapper": {
    "src/(.*)": "<rootDir>/src/$1"
  },
  "rootDir": "../",
  "modulePaths": ["node_modules","<rootDir>"],
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
