{
  "name": "eom-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "seed": "ts-node src/modules/prisma/seed.ts"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch --config ./test/jest-e2e.json",
    "pretest:cov": "npm run prepretest:e2e && dotenv -e .env.test ts-node test/seed.ts",
    "test:cov": "cross-env NODE_ENV=test jest --coverage --config ./test/jest-e2e.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./test/jest-e2e.json",
    "prepretest:e2e": "dotenv -e .env.test npx prisma migrate reset -- --skip-seed --force",
    "pretest:e2e": "npm run prepretest:e2e && dotenv -e .env.test ts-node test/seed.ts",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "test:e2e:findOpenHandles": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --detectOpenHandles ",
    "migration": "npx prisma migrate dev -- --preview-feature",
    "generate": "npx prisma generate -- --preview-feature",
    "refresh:database": "npx prisma migrate reset",
    "seeder": "npx prisma db seed",
    "prisma:format": "npx prisma format",
    "migration-status": "npx prisma migrate status"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/swagger": "^5.2.1",
    "@prisma/client": "^3.11.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/cron": "^2.0.0",
    "@types/lodash": "^4.14.183",
    "@types/multer": "^1.4.7",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "chalk": "4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-env": "^7.0.3",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.1.0",
    "handlebars": "^4.7.7",
    "html-pdf": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nodemailer": "^6.7.5",
    "number-to-words": "^1.2.4",
    "papaparse": "^5.3.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2",
    "winston": "^3.6.0",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^17.0.21",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "prisma": "^3.12.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
